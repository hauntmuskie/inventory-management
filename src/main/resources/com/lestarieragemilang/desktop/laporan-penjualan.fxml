               <font>
                  <Font size="14.0" />
               </font>
            </JFXButton>
            <Text layoutX="290.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0"
               text="LAPORAN PENJUALAN" AnchorPane.topAnchor="14.0">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Text>
            <TableView fx:id="sellTable" prefHeight="300.0" prefWidth="930.0"
