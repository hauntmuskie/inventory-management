   fx:controller="com.lestarieragemilang.desktop.controller.ReportSales">
   <children>
      <AnchorPane prefHeight="573.0" prefWidth="970.0">
         <children>
            <TextField fx:id="SellListSearchField" layoutX="360.0" layoutY="135.0" prefHeight="32.0"
               prefWidth="250.0" promptText="Cari Faktur" style="-fx-background-radius: 5;" />
            <JFXButton buttonType="RAISED" layoutX="435.0" layoutY="218.0"
               onMouseClicked="#printJasperSellList" prefHeight="30.0" prefWidth="107.0"
