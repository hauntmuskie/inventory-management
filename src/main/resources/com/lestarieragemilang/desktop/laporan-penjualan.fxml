                  <TableColumn fx:id="sellBrandCol" prefWidth="75.0" text="MEREK" />
                  <TableColumn fx:id="sellTypeCol" prefWidth="75.0" text="JENIS" />
                  <TableColumn fx:id="sellTotalCol" prefWidth="75.0" text="JUMLAH" />
                  <TableColumn fx:id="sellPriceCol" prefWidth="75.0" text="HARGA" />
                  <TableColumn fx:id="sellSubTotalCol" prefWidth="75.0" text="SUB TOTAL" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <DatePicker fx:id="SellListDateFirstField" layoutX="290.0" layoutY="175.0"
               prefHeight="32.0" promptText="Dari Tanggal" />
            <DatePicker fx:id="SellListDateSecondField" layoutX="506.0" layoutY="175.0"
               prefHeight="32.0" promptText="Sampai Tanggal" />
            <Text layoutX="375.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0"
               text="Note : Pilih salah satu pencarian, search / date">
               <font>
                  <Font name="System Bold Italic" size="10.0" />
               </font>
